AC_PREREQ(2.52)
AC_INIT(README)

AM_CONFIG_HEADER(config.h)

ACLOCAL_FLAGS="-I m4"
AC_SUBST(ACLOCAL_AMFLAGS, $ACLOCAL_FLAGS)

AM_INIT_AUTOMAKE(gnome-themes-extras, 0.6)

# Honor aclocal flags
ACLOCAL="$ACLOCAL $ACLOCAL_FLAGS"


GETTEXT_PACKAGE=gnome-themes-extra
AC_SUBST(GETTEXT_PACKAGE)

AM_MAINTAINER_MODE

AC_PROG_INTLTOOL([0.23])
AC_PROG_CC
AC_PROG_INSTALL

# No static please
AM_DISABLE_STATIC

# Initialize libtool
AM_PROG_LIBTOOL

PKG_CHECK_MODULES(GTK,
		  gtk+-2.0 >= 2.0.0,,
		AC_MSG_ERROR([GTK+-2.0 is required to compile gtk-engines]))

GTK_BINARY_VERSION=`$PKG_CONFIG --variable=gtk_binary_version gtk+-2.0`

AC_SUBST(GTK_CFLAGS)
AC_SUBST(GTK_LIBS)
AC_SUBST(GTK_BINARY_VERSION)

# Checks for header files.
AC_HEADER_STDC

# Checks for typedefs, structures, and compiler characteristics.
AC_C_CONST

AS_AC_EXPAND(DATADIR, $datadir)

# Checks for endianness (needed by GdkRgb).
AC_C_BIGENDIAN

# Checks for library functions.
AC_TYPE_SIGNAL
AC_FUNC_MMAP
AC_FUNC_ALLOCA
AC_CHECK_FUNCS(setenv putenv)

ALL_LINGUAS="az ca cs da de el es fr hr he it ja ko lt nl no pl pt sk sr sr@Latn sv tr zh_CN zh_TW"
AM_GLIB_GNU_GETTEXT

